page {
    background-color: #ecf0f3;
}

.banner {
    display: block;
    width: 750rpx;
    height: 320rpx;
}

.title-wrap {
    margin: 10rpx 19rpx 0rpx 35rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.title-wrap .logo-name {
    display: flex;
    align-items: center;
}

.title-wrap .logo-name .logo {
    width: 100rpx;
    height: 100rpx;
}

.title-wrap .logo-name .name {
    margin-left: 8rpx;
    font-size: 38rpx;
    color: rgba(217,33,41,1);
}

.title-wrap .button {
    width: 185rpx;
    height: 50rpx;
    border-radius: 50rpx;
    background-color: #da4240;
    color: #fff;
    font-size: 28rpx;
}

.title-wrap navigator.button {
    margin-bottom: 10rpx;
}

.title-wrap .button .icon,.title-wrap .button text {
    display: inline-block;
    vertical-align: middle;
}

.title-wrap .button .icon {
    margin: 6rpx 10rpx;
    width: 38rpx;
    height: 38rpx;
}

.common-block {
    margin: 10rpx 20rpx 0;
    padding: 0 16rpx;
    background-color: #fff;
    border-radius: 16rpx;
}

.common-title {
    display: flex;
    align-items: center;
    height: 54rpx;
    padding-left: 48rpx;
    background-size: 36rpx 36rpx;
    background-position: center left;
    background-repeat: no-repeat;
    color: #2e2e2e;
    font-size: 28rpx;
}

.common-block-list {
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
}

.common-block-list .common-block-wrap {
    width: 331rpx;
    height: 160rpx;
    margin-bottom: 10rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    border-radius: 12rpx;
    background-color: #5bbd28;
}

.common-block-list .common-block-wrap .icon {
    margin-top: 24rpx;
    margin-bottom: 12rpx;
    width: 102rpx;
    height: 82rpx;
}

.common-block-list .common-block-wrap .name {
    font-size: 24rpx;
    color: #2e2e2e;
}

.complaint-clues .common-title {
    background-image: url('data:img/jpg;base64,');
}

.legal-service {
    margin-bottom: 30rpx;
}

.legal-service .common-title {
    background-image: url('data:img/jpg;base64,');
}
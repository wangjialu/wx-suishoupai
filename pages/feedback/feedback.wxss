page {
    background-color: #ecf0f3;
}

.container {
    padding: 20rpx 20rpx 0;
}

.feedback-wrap {
    padding: 0 20rpx 20rpx;
    border-radius: 10rpx;
    background: #fff;
}

.feedback-wrap .title {
    height: 60rpx;
    line-height: 60rpx;
    padding-left: 54rpx;
    background-image: url('data:img/jpg;base64,');
    background-size: 36rpx 36rpx;
    background-position: center left;
    background-repeat: no-repeat;
    font-size: 30rpx;
    color: #2E2E2E;
}

.feedback-wrap .content {
    padding-top: 18rpx;
    padding-left: 18rpx;
    border: 2rpx solid #ECF0F3;
    border-radius: 10rpx;
}

.feedback-wrap .content textarea {
    padding-right: 30rpx;
    height: 156rpx;
    font-size: 22rpx;
    color: #8B8B8B;
}

.feedback-wrap .content .counter {
    padding-right: 20rpx;
    height: 26rpx;
    line-height: 26rpx;
    text-align: right;
    font-size: 20rpx;
    color: #8B8B8B;
}

.button {
    margin: 635rpx 10rpx 0;
    height: 88rpx;
    line-height: 88rpx;
    border-radius: 10rpx;
    background-color: #09BB07;
    text-align: center;
    font-size: 36rpx;
    color: #fff;
}
page {
    padding-bottom: 100rpx;
}

.tab-bar-wrap {
    position: fixed;
    left: 0;
    bottom: 0;
    display: flex;
    justify-content: space-between;
    width: 100%;
    height: 100rpx;
    padding: 12rpx 82rpx 0;
    box-sizing: border-box;
    box-shadow: 0rpx 1rpx 5rpx 0rpx rgba(121,121,121,1);
    background-color: #fff;
}

.tab-bar-wrap .tab-icon {
    width: 50rpx;
    height: 50rpx;
    background-size: 50rpx 50rpx;
    background-repeat: no-repeat;
}

.tab-bar-wrap .tab-name {
    margin-top: 5rpx;
    font-size: 22rpx;
    color: #9a9da1;
    text-align: center;
}

.tab-bar-wrap .tab-home .tab-icon {
    background-image: url('data:img/jpg;base64,');
}

.tab-bar-wrap .tab-complaint {
    margin-top: -33rpx;
}

.tab-bar-wrap .tab-complaint .tab-icon-wrap {
    width: 80rpx;
    height: 80rpx;
    border-radius: 50%;
    border: 5rpx solid #9a9da1;
    background-color: #fff;
    box-sizing: border-box;
}

.tab-bar-wrap .tab-complaint .tab-icon-wrap .tab-icon {
    margin: 10rpx;
    background-image: url('data:img/jpg;base64,');
}

.tab-bar-wrap .tab-complaint .tab-name {
    margin-top: 9rpx;
}

.tab-bar-wrap .tab-mine .tab-icon {
    background-image: url('data:img/jpg;base64,');
}

.tab-bar-wrap .tab-home.active .tab-icon {
    background-image: url('data:img/jpg;base64,');
}

.tab-bar-wrap .tab-complaint.active .tab-icon-wrap {
    border: 5rpx solid #69d98a;
}

.tab-bar-wrap .tab-complaint.active .tab-icon {
    background-image: url('data:img/jpg;base64,');
}

.tab-bar-wrap .tab-mine.active .tab-icon {
    background-image: url('data:img/jpg;base64,');
}

.tab-bar-wrap .active .tab-name {
    color: #69d98a;
}